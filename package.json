    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "matter-js": "^0.19.0",
    "nestjs-prisma": "^0.22.0",
    "nodemailer": "^6.9.4",
